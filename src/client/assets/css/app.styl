// *** !GLOBAL VARIABLES ***
// *** GRID ***
.row
  display: flex
  flex-flow: row wrap
  flex: 1 1 auto
  overflow: hidden
  margin-left: -($gap)
  margin-right: -($gap)
  &.row-no-wrap
    flex-wrap: nowrap
  &.row-reverse
    flex-direction: row-reverse
for $i in (1)..($col-count)
  .col-{$i}
    flex: 0 1 'calc(100% * %s / %s - %s * 2)' % ($i $col-count $gap)
    margin: $gap
  if $i < $col-count
    .col-skip-{$i}
      margin-left: 'calc(100% * %s / %s + %s)' % ($i $col-count $gap) !important
// *** !GRID ***
// *** MIXINS ***
button($color, $lighten = null)
  border-color: $color
  background-color: $color
  &:hover,
	&:focus,
	&:active
    if $lighten == null
      border-color: darken($color, 5%)
      background-color: darken($color, 5%)
    else
      border-color: rgba($color, 0.95)
      background-color: rgba($color, 0.95)
  &[disabled]
    cursor: not-allowed
    opacity: .85
    &:hover,
		&:active,
		&:visited
      border-color: $color
      background-color: $color
// *** !MIXINS ***
// *** COMMON CLASSES ***
.text
  &-center
    text-align: center
  &-left
    text-align: left
  &-right
    text-align: right
  &-disabled
    color: rgba($dark-secondary, 0.5) !important
  &-success
    color: $status-success !important
  &-danger
    color: $status-danger !important
  &-small
    font-size: 85%
.pull
  &-left
    float: left
  &-right
    float: right
.padding
  padding-top: ($gap * 2)
  padding-bottom: ($gap * 2)
  &-lg
    padding-top: ($gap * 5)
    padding-bottom: ($gap * 5)
.margin
  margin-top: ($gap * 2)
  margin-bottom: ($gap * 2)
  &-top
    margin-top: ($gap * 2)
  &-btm
    margin-bottom: ($gap * 2)
  &-top-lg
    margin-top: ($gap * 5)
  &-btm-lg
    margin-bottom: ($gap * 5)
// *** !COMMON CLASSES ***
// *** FONTS ***
$ff-primary = "Roboto", sans-serif
$fw-extrabold = 700
$fw-bold = 500
$fw-regular = 400
$fw-light = 300
$font-base = 14px
h1, .font-h1
  font: $fw-light $font-base * 8.5 / 1.2 $ff-primary
  letter-spacing: -0.01em
  text-indent: 0
  white-space: nowrap
h2, .font-h2
  font: $fw-light $font-base * 5.25 / 1.2 $ff-primary
  letter-spacing: -0.005em
  text-indent: 0
  white-space: nowrap
h3, .font-h3
  font: $fw-light $font-base * 3.25 / 1.2 $ff-primary
  letter-spacing: normal
  text-indent: 0
h4, .font-h4
  font: $fw-bold $font-base * 2 / 1.2 $ff-primary
  letter-spacing: normal
  text-indent: 0
h5, .font-h5
  font: $fw-bold $font-base * 1.24 / 1.5  $ff-primary
  letter-spacing: normal
  text-indent: 0
.font-b
  font: $fw-regular $font-base / 1.5 $ff-primary
  letter-spacing: 0.02em
  text-indent: 0
.font-f
  font: $fw-regular $font-base * 1.24 / 1.2 $ff-primary
  letter-spacing: normal
  text-indent: 0
// *** !FONTS ***
*
  font-smooth: always
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale
  box-sizing: border-box
html, body
  width: 100%
  height: 100%
  margin: 0
  padding: 0
.app
  @extends .font-b
  color: $text-dark
  min-width: $desktop-min-width
  display: flex
  flex-direction: column
  overflow: hidden
  width: 100%
  height: 100%
ul
  margin: 0
  &.nostyle
    list-style: none
    padding-left: 0
  &.inline
    @extends ul.nostyle
    li
      display: inline-block    
.btn
  display: inline-block
  border: 1px solid $brand-primary
  border-radius: ($gap / 1.25)
  outline: none
  padding: ($gap * 1.5) ($gap * 3.5)
  margin-bottom: ($gap / 2)
  color: $text-light
  text-decoration: none
  transition: all .1s ease-in
  box-shadow: 0 1px 2px 0 rgba($dark-secondary, 0.2)
  text-align: center
  cursor: pointer
  button($brand-primary)
  &.btn-wide
    width: 100%
  &.btn-small
    padding: ($gap / 1.25) ($gap * 2)
    border-radius: ($gap / 1.6)
    &.btn-select
      padding-right: ($gap * 3)
  &.btn-inverted
    button($text-light, true)
    color: $brand-primary
  &.btn-link
    background: transparent
    border-color: transparent
    box-shadow: none
    color: $brand-primary
  &.btn-select
    padding-right: ($gap * 4.5)
    &:after
      position: absolute
      content: backgroundSVG($svg-arrow-down)
      margin: -2px 0 0 4px
  &.btn-actions
    width: 35px
    height: ($gap * 0.8)
    margin: 0
    padding: ($gap * 1.5) 0
    background: none
    border: none
    box-shadow: none
    vertical-align: middle
    background-image: backgroundSVG($svg-actions)
    background-position: center center
    background-repeat: no-repeat
    background-size: contain
  &.btn-menu
    .popup
      display: none
      position: absolute
      text-align: left
      padding: 0
      margin: 8px 0 0 -26px
      border: 1px solid rgba($dark-secondary, 0.15)
      border-radius: ($gap / 5)
      box-shadow: 0 2px 4px 0 rgba($dark-secondary, 0.1)
      background-color: $light-primary
      color: $brand-primary
      max-width: 240px
      z-index: 2
      // This is a fix, delete it in production
      &.popup-last
        transform: translateX(-118px)
      .popup-menu
        padding: ($gap * 1.5) 0
        @extends ul.nostyle
        li
          padding: ($gap / 1.25) ($gap * 1.5)
          min-width: 180px
          transition: all .1s ease-in
          &:hover
            background-color: rgba($dark-secondary, 0.15)
          a
            display: block
            color: $dark-primary
          &:first-child
            margin-top: (-($gap) / 1.25)
          &:last-child
            margin-bottom: (-($gap) / 1.25)
          &.delimiter
            border-top: 1px solid rgba($dark-secondary, 0.15)
            padding: 0
            margin: ($gap / 2) 2px
      .form
        padding: ($gap * 1.5)
    &:hover
      .popup
        display: block
  &:not(.btn-link) + .btn
    margin-left: ($gap / 2)      
  &.btn-success
    button($status-success)
  &.btn-danger
    button($status-danger)
  &.btn-fb
    button($facebook)
  &.btn-gp
    button($google)
  &.btn-wl
    button($windows)
  &.btn-wl
    button($windows)
  &.btn-in
    button($linkedin)
  .icon-glyph
    height: ($gap * 2)
    width: ($gap * 2)    
a
  &:link,
	&:visited
    appearance: none
    outline: none
    cursor: pointer
    color: $brand-primary
    border: none
    background: transparent
    text-decoration: underline
  &:hover,
	&:focus,
	&:active
    text-decoration: none
  &.plain
    &:link,
		&:visited
      text-decoration: none
    &:hover,
		&:focus,
		&:active
      color: darken($brand-primary, 10%)
  &.block
    display: block
    margin: $gap auto
  &.btn
    @extends .btn
h1, h2, h3, h4, h5, h6
  margin-top: ($gap * 3)
  margin-bottom: $gap
p
  @extends .margin-top
  margin-bottom: $gap
.header,
.footer
  width: 100%
  height: 54px
  flex: 0 0 auto
  display: flex
  flex-flow: row nowrap
  padding: $gap
  .row
    flex-wrap: nowrap
    > div[class^=col]
      margin-top: 0
      margin-bottom: 0
  p
    margin: 0
  .brand
    @extends .font-h5
    a
      text-decoration: none
    &.brand-own
      a
        @extends .font-h4
        font-family: 'Syncopate'
        letter-spacing: ($gap / 1.25)
        text-transform: uppercase
        line-height: 40px
    img
      max-height: 100%
      width: auto
      margin-right: $gap
  .btn:not(.btn-icon)
    display: inline-block
    padding: ($gap / 2) ($gap * 2.5)
    cursor: pointer
    &:not(.btn-link)
      border-radius: ($gap / 1.6)
    &.btn-select
      padding-right: ($gap * 3)
  .form
    input:not([type="checkbox"]),
		select
      &.input-inverted
        display: inline-block
        padding: ($gap / 2) $gap
        border: 1px solid $text-light
        border-radius: ($gap / 1.6)
        box-shadow: none
        background: $text-light
        color: $text-dark
        @extends .font-b
        &:focus,
				&:active
          border-color: rgba(white, 1)
    &.search
      width: ($gap * 3.5)
      height: ($gap * 3.4)
      display: inline-block
      transition: all .1s ease-in
      input.input-inverted
        width: 100%
        height: 100%
        padding-left: ($gap * 4)
        background-size: auto 20px
        background-repeat: no-repeat
        background-position: left $gap center
        background-color: transparent
        background-image: backgroundSVG($svg-search) !important
        border: none
        cursor: pointer
        &::-webkit-input-placeholder
          color: rgba($dark-primary, 0.25)
        &::-moz-placeholder
          color: rgba($dark-primary, 0.25)
        &:-ms-input-placeholder
          color: rgba($dark-primary, 0.25)
        &:-moz-placeholder
          color: rgba($dark-primary, 0.25)
      &:hover
        width: 100%
        input
          background-color: $text-light
          border: 1px solid $text-light
          background-image: backgroundSVG($svg-search, $dark-primary) !important
    .controls-list > li > .btn
      @extends .btn
      margin-bottom: 0
.header
  background-color: $brand-primary
  color: $text-light
  &.header-transparent
    background-color: transparent
    color: $brand-primary
    height: 60px
  a.inverted
    line-height: ($gap * 3.5)
  .btn-link
    color: $text-light
    &:hover,
		&:focus,
		&:active
      background-color: darken($brand-primary, 5%)
  .btn-icon
    width: ($gap * 3.4)
    height: ($gap * 3.4)
    border-radius: 50%
    padding: 0
    box-shadow: none
    cursor: pointer
    color: $text-light
    text-decoration: none
    span
      line-height: ($gap * 3.4)
      font-weight: $fw-bold
.workspace
  display: flex
  overflow: hidden
  flex: 1 1 auto
  &.workspace-centered
    justify-content: center
    align-content: center
    .row
      justify-content: center
.sidebar
  width: 240px
  background-color: $dark-primary
  color: $text-light
  padding: $gap
  ul
    @extends ul.nostyle
    li
      padding: ($gap / 1.6) $gap
      transition: all .1s ease-in
      border-radius: ($gap / 2)
      &.active
        a
          font-weight: $fw-extrabold
          color: $light-primary
      &:hover
        background-color: rgba(white, 0.1)
      a
        display: flex
        flex-flow: row nowrap
        justify-content: space-between
        align-items: center
        color: $text-light
        &:link,
				&:focus,
				&:hover,
				&:active,
				&:visited
          text-decoration: none
        .label
          background-color: rgba(white, 0.1)
    &.no-highlight
      li:hover
        background: none
  h5
    padding: 0 $gap ($gap / 2)
    border-bottom: 1px solid rgba(white, 0.2)
    &:first-of-type
      margin-top: $gap
  &.sidebar-narrow
    width: 60px
    background-color: $dark-secondary
    padding: ($gap * 1.5) ($gap / 1.25)
    ul
      li
        display: block
        padding: $gap
        border-radius: ($gap / 1.6)
        &.active
          background-color: rgba(white, 0.1)
        a
          display: block
          width: 100%
          height: 24px
          /*					span {
          						display: none;
          					}*/
        /*				&:hover {
        					span {
        						position: absolute;
        						display: inline-block;
        						margin-left: 48px;
        					}
        				}*/
        &:not(:last-child)
          margin-bottom: $gap
  .user-status
    width: ($gap / 1.25)
    height: ($gap / 1.25)
    border-radius: 50%
    &.user-online
      background-color: $status-success
    &.user-offline
      background-color: rgba(white, 0.2)
.main
  flex: 1 1 auto
  overflow-y: scroll
  padding: ($gap * 3)
  //	background-color: rgba($dark-primary, .05);
  h3:first-of-type,
	h4:first-of-type
    font-weight: $fw-light
    margin: 0
    span
      @extends .font-f
    &:after
      display: block;
      content: getLength(hello world)
  .table
    width: 100%
    display: table
    background-color: $light-primary
    padding: $gap ($gap * 1.5)
    border: 1px solid rgba($dark-primary, 0.2)
    border-radius: ($gap / 1.6)
    overflow: hidden
    @extends ul.nostyle
    .t-row
      display: table-row
      &:not(.t-head)
        transition: all .1s ease-in
        &:hover
          background-color: rgba($dark-primary, 0.05)
      &.t-head
        @extends .text-small
        font-weight: $fw-extrabold
        .t-col
          border-bottom: 1px solid rgba($dark-primary, 0.2)
          padding-bottom: ($gap / 1.6)
          .icon-glyph
            margin-left: ($gap / 2)
      .t-col
        display: table-cell
        padding: ($gap * 1.25) $gap
        border-bottom: 1px solid rgba($dark-primary, 0.1)
        &:nth-child(4)
          cursor: pointer
          a
            color: inherit
            text-decoration: none
          &:hover
            a
              color: $brand-primary
              text-decoration: underline
      &:last-child
        .t-col
          border-bottom: none
    &.users-table
      border: none
      border-radius: 0
      padding: 0
      .user-icon
        width: ($gap * 3)
        height: ($gap * 3)
        @extends .font-b
        font-weight: $fw-bold
      .t-row
        .t-col
          cursor: default
          &:first-child
            width: ($gap * 5)
          &:nth-child(2)
            width: 30%
          &:nth-child(3)
            width: 25%
          &:nth-child(5)
            width: ($gap * 3)
        &:hover
          background: none
          .t-col:nth-child(3)
            color: $text-dark
        &.row-disabled
          opacity: .5
          &:hover
            opacity: 1
      .user-unverified
        display: inline-block
        width: ($gap * 2)
        height: ($gap * 2)
        vertical-align: middle
        background-image: backgroundSVG($svg-warning)
        background-position: center center
        background-repeat: no-repeat
        background-size: contain
        cursor: pointer
      .resend-email
        display: inline-block
        width: ($gap * 1.7)
        height: ($gap * 1.7)
        vertical-align: middle
        background-image: backgroundSVG($svg-resend)
        background-position: center center
        background-repeat: no-repeat
        background-size: contain
        cursor: pointer
  .panel
    @extends .main .table
    padding: ($gap * 2) ($gap * 1.5) ($gap * 1.5)
    &.user-card
      display: flex
      flex-flow: column nowrap
      align-items: center
      h5
        margin-top: ($gap * 1.5)
    h5:first-of-type
      margin-top: 0
  .caption
    margin: ($gap * 3.5) 0 ($gap * 1.5)
    padding-bottom: ($gap / 2)
    border-bottom: 1px solid rgba($dark-primary, 0.2)
    &:first-of-type
      margin-top: ($gap * 2)
  .ticket-subject
    margin-bottom: ($gap / 1.25)
    a.font-h4
      font-weight: $fw-light
      text-decoration: none
      color: $text-dark
      &:hover
        border-bottom: 1px dotted rgba($dark-primary, 0.5)
    span.text-disabled
      margin: 0 ($gap / 2)
    + ul
      li
        margin-right: $gap
    &.new-subject
      a.font-h4
        @extends .text-disabled
        border-bottom: 1px dotted rgba($dark-primary, 0.5)
  .ticket
    width: 100%
    background-color: $light-primary
    border: 1px solid rgba($dark-primary, 0.2)
    border-radius: ($gap / 1.6)
    overflow: hidden
    display: flex
    flex-flow: column nowrap
    .comment
      padding: ($gap * 1.5) ($gap * 1.5) ($gap * 1.5) ($gap * 2.5)
      display: flex
      flex-flow: row nowrap
      &:not(:first-of-type)
        border-top: 1px solid rgba($dark-primary, 0.2)
      .comment-user
        padding-right: ($gap * 1.5)
        .user-icon
          width: ($gap * 6)
          height: ($gap * 6)
          @extends .font-h5
          text-decoration: none
          color: $text-light
      .comment-text
        flex: 1 1 auto
        h5
          margin-top: 0
          font-weight: $fw-extrabold
          span
            margin-left: ($gap / 2)
            @extends .font-b
        .comment-attachment
          @extends a.plain
          display: inline-block
          margin-top: $gap
          padding: $gap $gap $gap ($gap * 3.5)
          border: 1px solid rgba($dark-primary, 0.1)
          border-radius: ($gap / 1.6)
          background-repeat: no-repeat
          background-position: left ($gap / 2) center
          background-size: 21px 24px
          background-image: backgroundSVG($svg-attachment)
          &.attachment-upload
            display: inline-flex
            flex-flow: row nowrap
            align-items: center
            position: relative
            background: none
            float: left
            clear: right
            color: $text-dark
            padding: ($gap / 2) $gap ($gap / 2) ($gap / 2)
            &:not(:last-of-type)
              margin-right: ($gap / 2.5)
            .upload-progress
              display: inline-flex
              flex-flow: row nowrap
              justify-content: center
              align-items: center
              height: ($gap * 3)
              width: ($gap * 3)
              border-radius: 50%
              margin-right: ($gap / 2)
              margin-top: 1px
              background-color: $brand-primary
              background-image: linear-gradient(91deg, transparent 50%, #ebeced 50%), linear-gradient(90deg, #ebeced 50%, transparent 50%)
              background-size: cover, cover
              for $i in (1)..(50)
                &.up-{$i}
                  background-image: linear-gradient(90 + $i * 3.6deg, transparent 50%, #ebeced 50%), linear-gradient(90deg, #ebeced 50%, transparent 50%)
              for $k in (51)..(99)
                &.up-{$k}
                  background-image: linear-gradient($k * 3.6 - 90deg, transparent 50%, $brand-primary 50%), linear-gradient(90deg, #ebeced 50%, transparent 50%)
              &.up-100
                background-image: linear-gradient(91deg, transparent 50%, $status-success 50%), linear-gradient(90deg, $status-success 50%, transparent 50%)
                .upload-cancel
                  background-image: backgroundSVG($svg-ok)
                &:hover
                  background-image: linear-gradient(91deg, transparent 50%, $c 50%), linear-gradient(90deg, $c 50%, transparent 50%)
                  .upload-cancel
                    background-image: backgroundSVG($svg-cancel, darken($brand-primary, 10%))
              .upload-cancel
                @extends a.plain
                width: ($gap * 2.6)
                height: ($gap * 2.6)
                border-radius: 50%
                display: flex
                flex-flow: row nowrap
                justify-content: center
                align-items: center
                font-size: 85%
                color: $brand-primary
                font-weight: $fw-extrabold
                letter-spacing: 0
                background-color: $light-primary
                background-image: backgroundSVG($svg-cancel)
                background-size: 50%
                background-position: center center
                background-repeat: no-repeat
                &:hover
                  background-image: backgroundSVG($svg-cancel, darken($brand-primary, 10%))
            .upload-item
              display: inline-flex
              flex-flow: column nowrap
              .upload-description
                color: rgba($dark-secondary, 0.5)
                font-size: 75%
        &.comment-reply
          .reply-formatting
            width: 100%
            height: 36px
            border: 1px solid rgba($dark-secondary, 0.25)
            border-top-left-radius: ($gap / 1.25)
            border-top-right-radius: ($gap / 1.25)
            border-bottom: none
            background-repeat: no-repeat
            background-size: contain
            background-image: url("/img/editor.png")
          textarea
            @extends .font-b
            min-height: ($gap * 10)
            border-top-left-radius: 0
            border-top-right-radius: 0
            resize: vertical
      &.note
        background-color: rgba($dark-primary, 0.05)
        @extends .text-disabled
.form
  input:not([type="checkbox"]),
	select,
	textarea
    border: 1px solid rgba($dark-secondary, 0.25)
    border-radius: ($gap / 1.25)
    outline: none
    padding: ($gap * 1.5)
    @extends .font-f
    color: $text-dark
    width: 100%
    transition: all .1s ease-in
    box-shadow: 0 1px 2px 0 rgba($dark-secondary, 0.1) inset
    margin: 0
    -webkit-appearance: none
    -moz-appearance: none
    appearance: none
    background: transparent
    &:focus,
		&:active
      border-color: rgba($dark-secondary, 0.5)
    &:disabled
      background-color: rgba($dark-primary, 0.05)
    &.input-valid
      border-color: rgba($status-success, 0.85)
    &.input-invalid
      border-color: rgba($status-danger, 0.85)
    &.input-small
      border-radius: ($gap / 1.6)
      padding: ($gap / 1.25)
      @extends .font-b
    &[id="domain"]
      + span
        @extends .font-f
        font-weight: $fw-bold
        position: absolute
        padding: ($gap * 1.5)
        right: 0
        margin-top: 1px
        background-color: rgba($dark-secondary, 0.05)
        border-top-right-radius: ($gap / 1.25)
        border-bottom-right-radius: ($gap / 1.25)
    &.no-resize
      resize: none
  select
    background-repeat: no-repeat
    background-position: right ($gap * 1.5) center
    background-size: 12px 5.25px
    vertical-align: middle
    margin-top: -2px
    background-image: backgroundSVG($svg-arrow-down, $dark-primary)
  label
    margin-bottom: ($gap / 2)
    color: rgba($dark-secondary, 0.5)
  input[type="checkbox"] + label
    margin-left: ($gap / 2)
    margin-bottom: 0
.controls-list
  width: 100%
  list-style: none
  margin: 0
  padding: 0
  > li
    position: relative
    display: flex
    flex-flow: row wrap
    align-items: center
    justify-content: space-between
    &:not(:first-child)
      @extends .margin-top
    p
      flex: 1 1 auto
    &.controls-centered
      justify-content: center
    &.controls-left
      justify-content: flex-start
.delimiter-col
  display: flex
  flex-flow: column nowrap
  justify-content: center
  align-items: center
  div
    display: block
    flex: 1 1 auto
    height: 100%
    width: 1px
    background-color: rgba($dark-primary, 0.25)
.center-col
  display: flex
  flex-flow: row wrap
  justify-content: center
  align-items: center
.icon
  display: inline-block
  height: ($gap * 4.2)
  width: auto
  vertical-align: middle
  margin-right: $gap
  &.icon-glyph
    background-repeat: no-repeat
    background-size: contain
    &.icon-fb
      background-image: backgroundSVG($svg-facebook)
    &.icon-wl
      background-image: backgroundSVG($svg-windows)
    &.icon-in
      background-image: backgroundSVG($svg-linkedin)
    &.icon-tickets
      background-image: backgroundSVG($svg-tickets)
    &.icon-kb
      background-image: backgroundSVG($svg-knowledgebase)
    &.icon-dashboard
      background-image: backgroundSVG($svg-dashboard)
    &.icon-settings
      background-image: backgroundSVG($svg-settings)
    &.icon-clip
      width: 14px
      height: 16.25px
      margin: 0
      background-image: backgroundSVG($svg-clip)
.label
  background-color: rgba(white, 0.1)
  border-radius: ($gap / 2.5)
  padding: 1px ($gap / 1.6)
  @extends .text-small
  background-color: rgba($dark-primary, 0.1)
  color: $text-light
  cursor: pointer
  &.label-inverted
    color: $text-dark
  &.priority
    font-weight: $fw-extrabold
    &.priority-high
      background-color: rgba($status-danger, 0.85)
    &.priority-normal
      background-color: rgba($brand-primary, 0.85)
    &.priority-low
      background-color: rgba($status-success, 0.85)
  &.label-loading
    width: ($gap * 4.25)
    height: ($gap * 2)
    padding: ($gap / 1.333)
    $dot
      display: block
      position: relative
      width: ($gap / 2)
      height: ($gap / 2)
      border-radius: 50%
      background-color: rgba(white, 0.2)
      margin: 0 auto
      animation: loadframes 1.2s infinite ease-in-out -0.3s  
    .dots
      @extends .label.label-loading $dot
      &:before,
      &:after
        @extends .label.label-loading $dot
        content: ''
        position: absolute
        top: 0
      &:before
        left: -($gap)
        animation-delay: -0.6s
      &:after
        left: $gap
        animation-delay: 0s
.user-icon
  position: relative
  width: ($gap * 10)
  height: ($gap * 10)
  border-radius: 50%
  padding: 0
  color: $text-light
  cursor: pointer
  display: flex
  flex-flow: row nowrap
  justify-content: center
  align-items: center
  @extends .font-h4
  font-weight: $fw-bold
  background-repeat: no-repeat
  background-size: cover
  background-position: center center
  overflow: hidden
  .change-photo
    position: absolute
    background: rgba($dark-secondary, 0.85)
    display: none
    padding: $gap
    width: 100%
    height: 100%
    top: 0
    left: 0
    @extends .font-b
    display: none
    flex-flow: row nowrap
    justify-content: center
    align-items: center
    text-align: center
    color: $text-light
  &:hover
    .change-photo
      display: flex
  &.org-icon
    background-size: 75%
.notifications
  display: flex
  flex-flow: column wrap
  align-items: flex-start
  position: fixed
  top: ($gap + 54px)
  right: $gap
  width: 360px
  overflow: hidden
  z-index: 999
  .notification
    position: relative
    width: 100%
    height: auto
    display: flex
    flex-flow: row nowrap
    justify-content: flex-start
    align-items: flex-start
    background-color: rgba($dark-secondary, 0.85)
    box-shadow: 0 1px 2px 0 rgba($dark-secondary, 0.2)
    border-radius: ($gap / 1.6)
    color: $text-light
    margin-left: 370px
    padding: ($gap * 1.5) ($gap * 4.5) ($gap * 1.5) ($gap * 1.5)
    opacity: .1
    transition: all .2s ease-in-out
    + .notification
      margin-top: $gap
    &.appeared
      margin-left: 0
      opacity: 1
    .notification-icon
      display: block
      width: ($gap * 4)
      height: ($gap * 4)
      margin-right: $gap
      background-image: backgroundSVG($svg-warning)
      background-position: center center
      background-repeat: no-repeat
      background-size: contain
      &.icon-error
        background-image: backgroundSVG($svg-error)
    .notification-body
      width: 100%
      height: 100%
      max-height: 105px
      overflow: hidden
      p
        margin: 0
    .notification-dismiss
      position: absolute
      width: ($gap * 2.6)
      height: ($gap * 2.6)
      top: ($gap / 2)
      right: ($gap / 2)
      border-radius: 50%
      background-image: backgroundSVG($svg-cancel, darken($light-primary, 75%))
      background-position: center center
      background-repeat: no-repeat
      background-size: 50%
      &:hover
        background-image: backgroundSVG($svg-cancel, darken($light-primary, 15%))
@keyframes loadframes
  0%,
  80%,
  100%
    box-shadow: 0 0 0 ($gap / 16) $text-light
    background-color: $text-light
  40%
    box-shadow: 0 0 0 0 rgba(white, 0.2)
    background-color: rgba(white, 0.2)
