  stroke: $mcyellow
.ct-chart-pie .ct-series.ct-series-a path
  fill: $mcyellow
.ct-chart-pie .ct-series.ct-series-b path
  fill: $status-danger
.ct-chart-pie .ct-series.ct-series-c path
  fill: $brand-primary
.ct-label
  color: #aaa
.ct-chart-pie .ct-label
  fill: $light-primary